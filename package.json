{
  "name": "mulheres-em-dados",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "preinstall": "npx npm-force-resolutions",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "refreshVSToken": "vsts-npm-auth -config \"./.npmrc\"",
    "refreshVSToken:wsl": "vsts-npm-auth -config \"./.npmrc\" -TargetConfig \"$HOME/.npmrc\"",
    "build:dev": "vite build --mode development && cp staticwebapp.config.json dist/staticwebapp.config.json",
    "build:stg": "vite build --mode staging && cp staticwebapp.config.json dist/staticwebapp.config.json",
    "build:prd": "vite build --mode production && cp staticwebapp.config.json dist/staticwebapp.config.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.21.5",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/systemjs": "^6.15.1",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^2.1.3",
    "axe-core": "^4.10.1",
    "cypress": "^13.15.0",
    "cypress-axe": "^1.5.0",
    "cypress-iframe": "^1.0.1",
    "cypress-xpath": "^2.0.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.9",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^2.1.3",
    "vitest-sonar-reporter": "^2.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  }
}
